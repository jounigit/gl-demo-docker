{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/app.js",
  "scripts": {
    "start": "node build/src/app.js",
    "dev:db": "docker compose -f docker-compose.db.yml up -d dbdev",
    "predev": "npm run dev:db",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/index.ts",
    "db:migrate": "npx prisma migrate dev --name user-entity --create-only && npx prisma generate",
    "db:push": "npx prisma db push",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test:db:up": "docker compose -f docker-compose.test.yml up -d dbtest",
    "test:db:down": "docker compose -f docker-compose.test.yml down dbtest",
    "pretest:i": "npm run test:db:up && npx prisma migrate deploy",
    "test:i": "NODE_ENV=test vitest run --config ./vitest.integration.ts",
    "posttest:i": "npm run test:db:down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-jwt": "^7.4.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.17",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "happy-dom": "^14.3.8",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.9.1",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.2.6",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "dotenv-cli": "^7.4.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^8.4.1",
    "http-errors": "^2.0.0",
    "image-size": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.13",
    "sharp": "^0.33.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
